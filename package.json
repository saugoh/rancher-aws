{
  "name": "rancher-aws",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/service.js"
  },
  "description": "Sample hello world template",
  "service": {
    "title": "Sample template to create services",
    "standalone" : true,
    "port" : 8080,
    "forever" : false
  },
  "apidoc": {
    "description": "apiDoc basic example",
    "title": "Custom apiDoc browser title",
    "url": "http://dsapi.dev.cly.np.top.corp.telstra.com/registry-dashboard/v1/apidoc/",
    "sampleUrl": "http://dsapi.dev.cly.np.top.corp.telstra.com/registry-dashboard/v1/apidoc/",
    "template": {
      "withCompare": false
    }
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  },
  "dependencies": {
    "aws-sdk" : "^2.2.34",
    "proxy-agent" : "^2.0.0",
    "async" : "^1.5.2",
    "moment": "^2.10.6",
    "node-validator": "^0.1.15",
    "request": "^2.53.0",
    "validator": "^4.0.5"
  },
  "devDependencies": {
    "apidoc": "^0.12.3",
    "chai": "^3.2.0",
    "gulp": "^3.9.0",
	"gulp-mocha": "^2.2.0",
	"gulp-istanbul": "^0.10.3",
	"gulp-tslint": "^4.2.2",
	"gulp-tslint-stylish": "^1.1.1",
    "gulp-bg": "^0.0.8",
    "del": "^2.2.0",
    "mocha": "^2.2.4",
    "mocha-bamboo-reporter": "^1.1.0"
  }
}
